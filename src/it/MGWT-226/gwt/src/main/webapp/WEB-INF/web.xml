<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  version="2.5">

  <servlet>
    <servlet-name>rpc1</servlet-name>
    <servlet-class>gwt.server.Rpc1ServiceImpl</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>rpc1</servlet-name>
    <url-pattern>/gwt.Application/rpc1</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>rpc2</servlet-name>
    <servlet-class>gwt.server.Rpc2ServiceImpl</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>rpc2</servlet-name>
    <url-pattern>/gwt.Application/rpc2</url-pattern>
  </servlet-mapping>

</web-app>
